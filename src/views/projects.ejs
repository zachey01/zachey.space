<!DOCTYPE html>
<html lang="en">
  <head>
    <%- include('../partials/head.ejs') %>
  </head>
  <body>
    <header><%- include('../partials/header.ejs') %></header>
    <main>
      <head>
        <link
          href="https://fonts.googleapis.com/css?family=Open+Sans:300i,400"
          rel="stylesheet"
        />
      </head>
      <body>
        <div class="card-container">
          <% json.forEach(function(project) { %>
          <div class="card">
            <a href="<%= project.link %>"
              ><h3 class="card-title"><%= project.name %></h3></a
            >
            <div class="bar"><%= project.description %></div>
          </div>
          <% }); %>
        </div>
      </body>
    </main>
  </body>
</html>
